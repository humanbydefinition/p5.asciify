{
  "name": "p5.asciify",
  "version": "0.9.0-beta.5",
  "description": "Apply real-time ASCII conversion to your favorite p5.js sketches instantly.",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/p5.asciify.umd.js",
  "module": "./dist/p5.asciify.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    }
  },
  "dependencies": {
    "p5": ">=1.8.0 <2.0.0"
  },
  "devDependencies": {
    "@types/p5": "^1.7.6",
    "@vitest/ui": "^3.1.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-glsl": "^1.3.1",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "22.15.0",
    "npm": "11.3.0"
  },
  "scripts": {
    "build": "vite build && tsc",
    "dev": "vite --host",
    "vitest": "vitest --ui",
    "build-docs": "typedoc",
    "dev-docs": "typedoc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humanbydefinition/p5-asciify.git"
  },
  "keywords": [
    "p5",
    "p5-library",
    "p5js",
    "ascii",
    "textmode"
  ],
  "author": "humanbydefinition",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/humanbydefinition/p5-asciify/issues"
  },
  "homepage": "https://github.com/humanbydefinition/p5-asciify",
  "files": [
    "dist"
  ]
}
