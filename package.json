{
  "name": "p5.asciify",
  "version": "0.9.1-beta.4",
  "description": "Apply real-time ASCII conversion to your favorite p5.js sketches instantly.",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/p5.asciify.umd.js",
  "module": "./dist/p5.asciify.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    },
    "./renderers": {
      "types": "./dist/types/renderers/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    },
    "./renderers/2d": {
      "types": "./dist/types/renderers/2d/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    },
    "./renderers/2d/feature": {
      "types": "./dist/types/renderers/2d/feature/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    },
    "./plugins": {
      "types": "./dist/types/plugins/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    },
    "./utils": {
      "types": "./dist/types/utils/index.d.ts",
      "import": "./dist/p5.asciify.esm.js",
      "require": "./dist/p5.asciify.umd.js"
    }
  },
  "typesVersions": {
    "*": {
      "renderers": [
        "./dist/types/renderers/index.d.ts"
      ],
      "renderers/2d": [
        "./dist/types/renderers/2d/index.d.ts"
      ],
      "renderers/2d/feature": [
        "./dist/types/renderers/2d/feature/index.d.ts"
      ],
      "plugins": [
        "./dist/types/plugins/index.d.ts"
      ],
      "utils": [
        "./dist/types/utils/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "p5": ">=1.8.0 <2.0.0"
  },
  "devDependencies": {
    "@types/p5": "^1.7.6",
    "@vitest/ui": "^3.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-glsl": "^1.3.1",
    "vitest": "^3.1.2"
  },
  "engines": {
    "node": "22.15.0",
    "npm": "11.3.0"
  },
  "scripts": {
    "build": "vite build && tsc",
    "dev": "vite --host --port 5175",
    "vitest": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humanbydefinition/p5.asciify.git"
  },
  "keywords": [
    "p5",
    "p5-library",
    "p5js",
    "ascii",
    "textmode"
  ],
  "author": "humanbydefinition",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/humanbydefinition/p5.asciify/issues"
  },
  "homepage": "https://p5.textmode.art",
  "files": [
    "dist"
  ]
}
